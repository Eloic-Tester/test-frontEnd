{
    "version": 2,
    "builds": [
      {
        "src": "dist/<nom-du-projet>/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/edit-user/:id",
        "dest": "/index.html",
        "renderMode": "dynamic"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }